{
  "name": "zodworksdev",
  "version": "2.0.0",
  "private": true,
  "author": "jason mcelhenney <hello@zodworks.dev>",
  "scripts": {
    "prebuild": "rimraf .next",
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env production",
    "deploy:dev": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "dev": "wrangler dev --local --port 3000",
    "dev:remote": "wrangler dev --remote --port 3000",
    "lint": "next lint",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:migrate:dev": "wrangler d1 execute portfolio-dev --command=\"$(cat prisma/migrations/20250119_remove_auth_tables/migration.sql)\"",
    "db:migrate:prod": "wrangler d1 execute portfolio-prod --command=\"$(cat prisma/migrations/20250119_remove_auth_tables/migration.sql)\"",
    "db:setup:dev": "npm run db:migrate:dev",
    "db:setup:prod": "npm run db:migrate:prod",
    "content:sync": "tsx scripts/sync-content.ts",
    "content:sync:blog": "tsx scripts/sync-content.ts blog",
    "content:sync:projects": "tsx scripts/sync-content.ts projects"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
